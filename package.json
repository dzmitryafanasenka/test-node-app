{
  "name": "node-js-blog-api",
  "version": "1.0.1",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose start",
    "dev": "docker build . -t jaymorrison/test-node-app && docker-compose up",
    "lint": "eslint --fix --config=.eslintrc './src/**/*.js'",
    "stop": "docker-compose stop",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "db:migrate": "npx sequelize db:migrate",
    "db:migrate:undo": "npx sequelize db:migrate:undo:all",
    "db:restore": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate"
  },
  "author": "Igor Lyatskiy",
  "contributors": [
    {
      "name": "Dzmitry Afanasenka",
      "email": "dzmitry.afanasenka@gmail.com"
    }
  ],
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-plugin-sort-requires": "^2.1.0",
    "nodemon": "^2.0.13"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "i": "^0.3.7",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "nodemailer": "^6.6.5",
    "npm": "^7.24.1",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^5.4.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "yamljs": "^0.3.0"
  }
}
